{
    "name": "ecommerce-application",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --host",
        "build": "tsc && vite build",
        "build:dev": "tsc && vite build --mode development",
        "lint": "eslint . --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "prepare": "cd ../ && husky install ./eCommerce-Application/.husky",
        "preview": "vite preview",
        "prettier": "npx prettier . --write",
        "test": "vitest --coverage",
        "deploy": "vercel --prod"
    },
    "dependencies": {
        "@commercetools/platform-sdk": "^4.11.0",
        "@commercetools/sdk-client-v2": "^2.2.0",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.14.3",
        "@mui/joy": "^5.0.0-beta.1",
        "@mui/material": "^5.14.5",
        "isomorphic-fetch": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.15.0",
        "react-spinners": "^0.13.8",
        "swiper": "^10.2.0",
        "vercel": "^31.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.22.9",
        "@babel/preset-env": "^7.22.9",
        "@babel/preset-react": "^7.22.5",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/react": "^18.2.15",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "@vitejs/plugin-react": "^4.0.3",
        "@vitest/coverage-v8": "^0.34.2",
        "eslint": "^8.2.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "husky": "^8.0.3",
        "prettier": "3.0.1",
        "sass": "^1.64.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "vite": "^4.4.5",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.34.2"
    }
